{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.0.0",
    "generators": {
      "simple": {
        "generatorName": "typescript-axios",
        "inputSpec": "./openapi/simple/openapi.yaml",
        "output": "./gen/simple"
      },
      "apikey": {
        "generatorName": "typescript-axios",
        "inputSpec": "./openapi/apikey/openapi.yaml",
        "output": "./gen/apikey"
      }
    }
  }
}
